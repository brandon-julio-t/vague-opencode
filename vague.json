    "vagueBg": "#141415",
    "vagueInactiveBg": "#1c1c24",
    "vagueFg": "#cdcdcd",
    "vagueFloatBorder": "#878787",
    "vagueLine": "#252530",
    "vagueComment": "#606079",
    "vagueBuiltin": "#b4d4cf",
    "vagueFunc": "#c48282",
    "vagueString": "#e8b589",
